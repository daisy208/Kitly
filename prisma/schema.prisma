generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Bundle {
  id        Int      @id @default(autoincrement())
model YourModelName {
  id    Int     @id @default(autoincrement())
  shop  String

  @@index([shop]) 
}
  title     String
  products  String
  createdAt DateTime @default(now())
}
